    "@types/react-dom": "^18.2.18",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/qrcode": "^1.5.5",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/cookie": "^0.6.0",
    "typescript": "^5.3.3",
    "prisma": "^5.9.0",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.33",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "ts-node": "^10.9.2",
