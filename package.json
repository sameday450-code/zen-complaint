{
  "name": "complaint-reporting-system",
  "version": "1.0.0",
  "description": "Production-ready real-time complaint reporting system with QR codes and AI voice assistant",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "network-ip": "node get-network-ip.js",
    "server": "ts-node --project tsconfig.server.json server/index.ts",
    "server:dev": "nodemon --exec ts-node --project tsconfig.server.json server/index.ts",
    "db:generate": "prisma generate",
