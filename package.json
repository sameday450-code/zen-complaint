    "db:studio": "prisma studio",
    "db:seed": "ts-node --project tsconfig.server.json prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.server.json prisma/seed.ts"
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@prisma/client": "^5.9.0",
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.3",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "helmet": "^7.1.0",
    "aws-sdk": "^2.1544.0",
    "elevenlabs": "^0.8.1",
